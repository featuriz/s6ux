{
    "controllers": {
        "@symfony/ux-chartjs": {
            "chart": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@symfony/ux-cropperjs": {
            "cropper": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "cropperjs/dist/cropper.min.css": true,
                    "@symfony/ux-cropperjs/dist/style.min.css": true
                }
            }
        },
        "@symfony/ux-lazy-image": {
            "lazy-image": {
                "enabled": true,
                "fetch": "eager"
            }
        }
    },
    "entrypoints": []
}
